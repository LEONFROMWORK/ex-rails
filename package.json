{
  "name": "excelapp-rails",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered Excel analysis and generation platform",
  "scripts": {
    "build": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/tailwind.css --minify",
    "build:dev": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/tailwind.css",
    "watch": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/tailwind.css --watch",
    "lint:js": "eslint app/javascript/**/*.js",
    "lint:js:fix": "eslint app/javascript/**/*.js --fix",
    "format": "prettier --write app/javascript/**/*.js",
    "test:js": "echo 'JavaScript tests not configured yet'",
    "analyze": "npx webpack-bundle-analyzer dist/stats.json",
    "audit": "npm audit --audit-level moderate",
    "clean": "rm -rf app/assets/builds/* node_modules/.cache tmp/cache/assets",
    "prepare": "echo 'Setting up development environment...'"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vueuse/core": "^13.5.0",
    "axios": "^1.10.0",
    "chart.js": "^4.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.525.0",
    "pinia": "^3.0.3",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "^3.3.1",
    "vue": "^3.4.0",
    "vue-i18n": "^9.14.5",
    "vue-router": "4"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.4.19",
    "vite-plugin-ruby": "^5.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not dead",
    "> 0.2%",
    "last 2 versions"
  ]
}
